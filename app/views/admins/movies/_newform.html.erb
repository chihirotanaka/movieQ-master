<%= form_for(movie, url: admins_movies_path) do |f| %>

    <label>画像</label><br />
    <%= f.attachment_field :image %>
    <label>お題</label><br />
    <%= f.collection_select :theme_id, @themes, :id, :theme %><br />
    <label>制作年</label><br />
    <%= f.select :year, Movie.years.keys, :selected=>Movie.years[movie.year] %><br />
    <label>タイトル</label><br />
    <%= f.text_field :title %><br />
    <label>コラム</label><br />
    <%= f.text_area :column %><br />
    <%= f.submit '作成',class: "btn btn-defalte"%><br />

<% end %>