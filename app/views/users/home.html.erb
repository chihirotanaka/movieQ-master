<h2>クイズ一覧</h2>
<!-- クイズ一覧 -->
<div class="element">
    <table class="table table-default">
      <tr>
        <th>No.</th>
        <th>絵文字😄</th>
        <th>Question</th>
        <th>閲覧数</th>
        <th colspan="2">評価</th>
      </tr>
      <% @quizzes.each do |q| %>
      <tr>
          <td><%= q.id %></td>
          <td><%= q.emoji%></td>
          <td><%= q.question%></td>
          <td>閲覧数<%= %></td>
          <td>👍<%= q.favorites.count %></td>
          <td>👎<%= q.unfavories.count %></td>
      </tr>
      <% end %>
    </table>
</div>